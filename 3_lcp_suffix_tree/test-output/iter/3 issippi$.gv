// Suffix tree
digraph {
	"|" [label=""]
	"$|$" [label="$"]
	"|" -> "$|$" [label="$"]
	"i|i" [label=i]
	"$|i$" [label="i$"]
	"i|i" -> "$|i$" [label="$"]
	"ppi$|ippi$" [label="ippi$"]
	"i|i" -> "ppi$|ippi$" [label="ppi$"]
	"ssippi$|issippi$" [label="issippi$"]
	"i|i" -> "ssippi$|issippi$" [label="ssippi$"]
	"|" -> "i|i" [label=i]
}
