// Suffix tree
digraph {
	"|" [label=""]
	"$|$" [label="$"]
	"|" -> "$|$" [label="$"]
	"as|as" [label=as]
	"ass$|asass$" [label="asass$"]
	"as|as" -> "ass$|asass$" [label="ass$"]
	"s|ass" [label=ass]
	"$|ass$" [label="ass$"]
	"s|ass" -> "$|ass$" [label="$"]
	"asass$|assasass$" [label="assasass$"]
	"s|ass" -> "asass$|assasass$" [label="asass$"]
	"as|as" -> "s|ass" [label=s]
	"|" -> "as|as" [label=as]
	"s$|s$" [label="s$"]
	"|" -> "s$|s$" [label="s$"]
}
