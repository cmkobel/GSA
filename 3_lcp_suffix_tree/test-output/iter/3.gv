// Suffix tree
digraph {
	"|" [label=""]
	"$|$" [label="$"]
	"|" -> "$|$" [label="$"]
	"i|i" [label=i]
	"$|i$" [label="i$"]
	"i|i" -> "$|i$" [label="$"]
	"ppi$|ippi$" [label="ippi$"]
	"i|i" -> "ppi$|ippi$" [label="ppi$"]
	"ssi|issi" [label=issi]
	"ppi$|issippi$" [label="issippi$"]
	"ssi|issi" -> "ppi$|issippi$" [label="ppi$"]
	"i|i" -> "ssi|issi" [label=ssi]
	"|" -> "i|i" [label=i]
}
