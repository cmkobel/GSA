// Suffix tree
digraph {
	"|" [label=""]
	"$|$" [label="$"]
	"|" -> "$|$" [label="$"]
	"i|i" [label=i]
	"$|i$" [label="i$"]
	"i|i" -> "$|i$" [label="$"]
	"i$|ii$" [label="ii$"]
	"i|i" -> "i$|ii$" [label="i$"]
	"ppi$|ippi$" [label="ippi$"]
	"i|i" -> "ppi$|ippi$" [label="ppi$"]
	"|" -> "i|i" [label=i]
}
