// Suffix tree
digraph {
	"|" [label=""]
	"a|a" [label=a]
	"b|ab" [label=ab]
	"baab$|abbaab$" [label="abbaab$"]
	"b|ab" -> "baab$|abbaab$" [label="baab$"]
	"$|ab$" [label="ab$"]
	"b|ab" -> "$|ab$" [label="$"]
	"a|a" -> "b|ab" [label=b]
	"ab$|aab$" [label="aab$"]
	"a|a" -> "ab$|aab$" [label="ab$"]
	"|" -> "a|a" [label=a]
	"b|b" [label=b]
	"baab$|bbaab$" [label="bbaab$"]
	"b|b" -> "baab$|bbaab$" [label="baab$"]
	"aab$|baab$" [label="baab$"]
	"b|b" -> "aab$|baab$" [label="aab$"]
	"$|b$" [label="b$"]
	"b|b" -> "$|b$" [label="$"]
	"|" -> "b|b" [label=b]
	"$|$" [label="$"]
	"|" -> "$|$" [label="$"]
}
